{
    "version": 2,
    "builds": [
        {
            "dest": "src/main.js",
            "use": "@vercel/node"
        }
    ],
    "outputDirectory": "src",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}