{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        }
    ],
    "crons": [
      {
        "path": "/api/v1/scheduler/runReminder",
        "schedule": "0 2 * * *"
      },
      {
        "path": "/api/v1/scheduler/runReminder",
        "schedule": "0 4 * * *"
      },
      {
        "path": "/api/v1/scheduler/runReminder",
        "schedule": "0 6 * * *"
      },
      {
        "path": "/api/3/scheduler/runReminder",
        "schedule": "0 8 * * *"
      }
    ]
}